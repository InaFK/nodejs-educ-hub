{
  "name": "node-crud-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "webpack --config webpack.config.js",
    "start:multi": "cross-env NODE_ENV=production node dist/cluster.js",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "start": "npm run start:dev"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.45.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
